{
    "manifest_version": 3,
    "name": "DeepWiki GitHub Redirector",
    "version": "1.0",
    "description": "Adds a button to GitHub repositories to redirect to DeepWiki.",
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://github.com/*/*"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }